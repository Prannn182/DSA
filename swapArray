package pranali_DSA;

import java.util.Arrays;

public class swap {
	public static void main(String[] args)
	{
		int arr[]= {1,2,3,4,5};
		swap(arr,1,3);
		System.out.println(Arrays.toString(arr));
		
	}
	static void swap(int array[],int index1,int index2) {
		int temp=array[index1];
		array[index1]=array[index2];
		array[index2]=temp;
		
	}

}
