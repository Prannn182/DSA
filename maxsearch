package pranali_DSA;

public class maxsearch {
   public static void main(String[] args) {
	   int[] arr= {12,34,56,78,90,100,786,8,-9,-1,-2};
	   System.out.println(max(arr));
   }
	   
static int max(int[] arr) {
	int ans=arr[0];
	for(int i=0;i<arr.length;i++) {
		if(arr[i]>ans)
		{
			ans=arr[i];
		}
	}return ans;
}	   
   
}
